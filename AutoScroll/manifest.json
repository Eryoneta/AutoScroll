{
	"manifest_version": 3,
	"name": "AutoScroll",
	"version": "1.0.0",
	"author": "Yo",
	"description": "Customizes and adds useful functions to the autoscroll.",
	"icons": {
		"16": "Icons/Icone16.png",
		"32": "Icons/Icone32.png",
		"48": "Icons/Icone48.png",
		"128": "Icons/Icone128.png"
	},
	"action": {
		"default_icon": {
			"32": "Icons/Icone32.png"
		}
	},
	"background": {
		"service_worker": "service.js"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"autoScrollRoot.js",
				"autoScrollView.js",
				"autoScrollFlow.js",
				"injection.js"
			],
			"all_frames": false,
			"run_at": "document_start"
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"Scrolls/*.png"
			],
			"matches": [
				"<all_urls>"
			]
		}
	]
}